# Server Configuration
NODE_ENV=development
PORT=3000
HOST=localhost

# File Upload Configuration
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=image/png,image/jpeg,image/jpg,image/webp

# Rate Limiting
